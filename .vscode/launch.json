{
    "configurations": [
    {
        "name": "(gdb) Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/executable/scope.out",
        "args": [
            "-q", "${workspaceFolder}/exp/pattern_graph/6voc/384.txt",
            // "-q", "${workspaceFolder}/exp/pattern_graph/5voc/30.txt",

            // "-t", "${workspaceFolder}/exp/data_graph/web-spamt.bin",

            // "-d", "${workspaceFolder}/exp/data_graph/small.txt",
            "-d", "${workspaceFolder}/exp/data_graph/web-spam.txt",

            // "-r", "${workspaceFolder}/result/5voc/small/30.txt",
            // "-r", "${workspaceFolder}/result/6voc/small/384.txt",
            "-r", "${workspaceFolder}/result/6voc/web-spam/384.txt",

        ],
        "stopAtConnect": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Set disassembly-flavor intel",
                "text": "set disassembly-flavor intel",
                "ignoreFailures": true
            }
        ],
    }
    ]
}